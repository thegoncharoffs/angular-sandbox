{
  "name": "angular-sandbox",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "hmr": "ng serve --configuration hmr -o",
    "build": "ng build",
    "prod": "ng build --prod",
    "server": "nodemon -r esm server/server.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "svg": "gulp svgstore",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "puppeteer-ssr": "node server/ssr-puppeteer/index.js",
    "auth-server": "nodemon --watch './server/**/*.ts' --exec ts-node ./server/authentification/server.ts",
    "build:client-and-server-bundles": "ng build --prod && ng run angular-sandbox:server:production --bundleDependencies all"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.4",
    "@angular/common": "^9.0.4",
    "@angular/compiler": "^9.0.4",
    "@angular/core": "^9.0.4",
    "@angular/forms": "^9.0.4",
    "@angular/platform-browser": "^9.0.4",
    "@angular/platform-browser-dynamic": "^9.0.4",
    "@angular/platform-server": "~9.0.4",
    "@angular/router": "^9.0.4",
    "@angular/service-worker": "~9.0.4",
    "@ngrx/effects": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "@ngrx/store-devtools": "^8.6.0",
    "@nguniversal/express-engine": "^8.2.6",
    "@nguniversal/module-map-ngfactory-loader": "v8.2.6",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "connect-multiparty": "^2.2.0",
    "cors": "^2.8.5",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.7-alpha.3",
    "express-jwt": "^5.3.3",
    "gulp": "^4.0.2",
    "jwks-rsa": "^1.8.0",
    "normalize.css": "^8.0.1",
    "puppeteer": "^2.0.0",
    "rxjs": "^6.5.4",
    "ts-node-dev": "^1.0.0-pre.49",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.4",
    "@angular/cli": "^9.0.4",
    "@angular/compiler-cli": "^9.0.4",
    "@angular/language-service": "^9.0.4",
    "@angularclass/hmr": "^2.1.3",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.0",
    "@types/express-fileupload": "^1.1.3",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.13",
    "codelyzer": "^5.1.2",
    "gulp-cheerio": "^0.6.3",
    "gulp-inject": "^5.0.5",
    "gulp-svgmin": "^2.2.0",
    "gulp-svgstore": "^7.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "mz": "^2.7.0",
    "nodemon": "^2.0.4",
    "protractor": "^5.4.3",
    "ts-loader": "^5.2.0",
    "ts-node": "^8.10.2",
    "tslint": "~5.15.0",
    "typescript": "^3.7.5",
    "webpack-cli": "^3.1.0"
